<h1>Welcome back to Bandroid, oh music-lover! Time to log in now, please.</h1>

<%= form_tag sessions_path do %>
  <%= label_tag :email %>
  <%= email_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "I'M SO HAPPY TO BE BACK, LEMME IN!"%>
<% end %>
