<h1>New Musician Form</h1>

<%= form_for(@musician) do |f| %>
  <%= f.label :stage_name %>
  <%= f.text_field :stage_name %> <br>
  <%= f.label :location, "Which city are you looking to join a band in?" %>
  <%= f.text_field :location %> <br>
  <%= f.label :bio%>
  <%= f.text_field :bio %> <br>

  <%= f.label "Select your instruments: "%><br>
  <%= f.collection_check_boxes :instrument_ids, @instruments, :id, :name%><br>
  <%= f.submit "I MAKE MUSIC, LET ME SHINE!" %>
<% end %>
